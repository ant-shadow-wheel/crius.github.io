[{
  "selector": "node",
  "style": {
    "width": 5,
    "height": 5,
    "background-color": "#666",
    "color": "black",
    "border-width": 0,
    "outline-width":0,
    "padding":0
  }
},
{
  "selector": "edge",
  "style": {
    "width": 2,
    "line-color": "#5c3917",
    "curve-style": "bezier"
  }
},
{
  "selector": ".closedloop",
  "style": {
    "line-color": "#000",
    "width": 1
  }
},
{
  "selector": ".arrowedge",
  "style": {
    "width": 2,
    "line-color": "#000",
    "target-arrow-color": "#000",
    "target-arrow-shape": "triangle",
    "curve-style": "bezier"
  }
},
{
  "selector": ".locationnode",
  "style": {
    "label":"data(label)",
    "width": 5,
    "height": 5,
    "background-color": "#edac28",
    "color": "#ffffff",
    "font-size": 8,
    "text-outline-color": "#613203",
    "text-outline-width": 1,
    "border-width": 0,
    "outline-width":0,
    "padding":0
  }
},
{
  "selector": ".center-right",
  "style": {
    "text-valign": "center",
    "text-halign": "right"
  }
},
{
  "selector": ".nonvisible",
  "style": {
    "visibility": "hidden",
    "display": "none"
  }
},
{
  "selector": ".yesvisible",
  "style": {
    "visibility": "visible",
    "display": "element"
  }
}
]