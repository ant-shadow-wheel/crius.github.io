[{
  "selector": "node",
  "style": {
    "width": 5,
    "height": 5,
    "background-color": "#666",
    "color": "black",
    "border-width": 0,
    "outline-width":0,
    "padding":0
  }
},
{
  "selector": "edge",
  "style": {
    "width": 2,
    "line-color": "#ccc",
    "curve-style": "bezier"
  }
},
{
  "selector": ".arrowedge",
  "style": {
    "width": 2,
    "line-color": "#000",
    "target-arrow-color": "#000",
    "target-arrow-shape": "triangle",
    "curve-style": "bezier"
  }
},
{
  "selector": "#tbug",
  "style": {
    "background-image" : "../assets/tbugmini.jpg",
    "background-fit" : "cover"
  }
}]